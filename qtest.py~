from libs.qrtpcrlib import *
import glob



DATAFILES = glob.glob('*.csv')

GROUPBYS = ['plate', 'pool']
USEAVGS = ['points', 'avg']
POINTS = ['allpoints']

for points in POINTS:
    for groupby in GROUPBYS:
        d = {}
        for fname in DATAFILES:
            loadscdata(d, fname)
            for useavg in USEAVGS:
                getsc(d, useavg, points, groupby)
                
